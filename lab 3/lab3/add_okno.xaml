<Window x:Class="lab3.add_okno"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lab3"
        mc:Ignorable="d"
        Title="add_okno" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="imie_nazwisko" HorizontalAlignment="Left" Margin="99,180,0,0" Text=" " TextWrapping="Wrap" VerticalAlignment="Top" Width="402" FontSize="30" Height="61" TextChanged="imie_nazwisko_TextChanged"/>
        <TextBox x:Name="counting" HorizontalAlignment="Left" Margin="99,338,0,0" Text=" " TextWrapping="Wrap" VerticalAlignment="Top" Width="232" FontSize="30" Height="61" TextChanged="counting_TextChanged"/>
        <Button x:Name="add2" Content="ADD" HorizontalAlignment="Left" Margin="630,74,0,0" VerticalAlignment="Top" FontSize="30" Click="add2_Click"/>
        <Label x:Name="tekst1" Content="Name:" HorizontalAlignment="Left" Margin="99,132,0,0" VerticalAlignment="Top" FontSize="25"/>
        <Label x:Name="tekst2" Content="Count (number):" HorizontalAlignment="Left" Margin="99,290,0,0" VerticalAlignment="Top" FontSize="25"/>

    </Grid>
</Window>
